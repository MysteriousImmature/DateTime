{
    "manifest_version": 3,
    "name": "DateTime",
    "version": "5.0",
    "description": "Enhanced date and time display with customizable calendars, themes, timezones, and event tracking",
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
    },
    "browser_specific_settings": {
        "gecko": {
          "id": "DateTime@Auraaid.netlify.com",
          "strict_min_version": "91.0"
        }
    }
}
  
